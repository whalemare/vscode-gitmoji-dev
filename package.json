{
	"name": "gitmoji-dev",
	"displayName": "Gitmoji Dev",
	"description": "An emoji guide for your commit messages!",
	"license": "MIT",
	"homepage": "https://github.com/whalemare/vscode-gitmoji-dev/blob/master/README.md",
	"author": {
		"name": "whalemare"
	},
	"repository": {
		"url": "https://github.com/whalemare/vscode-gitmoji-dev.git",
		"type": "git"
	},
	"publisher": "whalemare",
	"version": "1.0.2",
	"engines": {
		"vscode": "^1.40.0"
	},
	"keywords": [
		"gitmoji",
		"git",
		"emoji",
		"commit",
		"messages",
		"gitmoji-dev",
		"gitmojidev",
		"whalemare"
	],
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:gitmoji-dev.commit",
		"onCommand:gitmoji-dev.commit-code"
	],
	"icon": "icon.gif",
	"galleryBanner": {
		"color": "#FFDD4C",
		"theme": "dark"
	},
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gitmoji-dev.commit",
				"title": "Commit with gitmoji ðŸ˜Š",
				"category": "Git"
			},
			{
				"command": "gitmoji-dev.commit-code",
				"title": "Commit with gitmoji :code:",
				"category": "Git"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "yarn run test-compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"changelog": "gitmoji-changelog",
		"release": "yarn ts-node ./scripts/release.ts",
		"deploy": "vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "14.x",
		"@types/shelljs": "^0.8.8",
		"@types/vscode": "^1.40.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"gh-release": "^6.0.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"ts-loader": "^8.0.14",
		"ts-node": "^10.0.0",
		"typescript": "^4.1.3",
		"vsce": "^1.92.0",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"execa": "^5.0.1"
	}
}